<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Student Data Table</title>
  <style>
    table {
      width: 100%;
      border-collapse: collapse;
    }
    table, th, td {
      border: 1px solid black;
    }
    th, td {
      padding: 10px;
      text-align: left;
    }
    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
  <h1>Student Data Table</h1>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Address</th>
        <th>Institution</th>
        <th>Batch</th>
        <th>Phone</th>
        <th>Facebook</th>
      </tr>
    </thead>
    <tbody>
      <% students.forEach(student => { %>
        <tr>
          <td><%= student.Name %></td>
          <td><%= student.email %></td>
          <td><%= student.address %></td>
          <td><%= student.Institution %></td>
          <td><%= student.batch %></td>
          <td><%= student.Phone %></td>
          <td><a href="<%= student.FbLink %>"><%= student.FbName %></a></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>